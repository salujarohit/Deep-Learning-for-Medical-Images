{
  "lr": 0.0001,
  "batch_size": 8,
  "epochs": 150,
  "batch_norm": true,
  "dropout": 0.5,
  "optimizer": "Adam",
  "loss": "dice_loss",
  "metrics": ["dice_coef", "precision", "recall"],
  "model": "unet",
  "base": 16,
  "input_shape": [256, 256, 1],
  "data_path": "/Lab1/Lab3/CT/",
  "test_size": 0.2,
  "last_layer_units": 3,
  "last_layer_activation": "softmax",
  "binarize_values": [251, 156],
  "unify_dict": {"156": "255", "251": "512" },
  "generator": {
    "rescale": "1.0 / 255.0 ",
    "rotation_range": 10,
    "width_shift_range": 0.1,
    "height_shift_range": 0.1,
    "horizontal_flip": true,
    "zoom_range": 0.2
  },
  "test_generator": {"rescale": "1.0 / 255.0"}
}