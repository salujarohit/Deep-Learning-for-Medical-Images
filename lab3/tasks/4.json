{
  "lr": 0.0001,
  "batch_size": 8,
  "epochs": 150,
  "batch_norm": false,
  "dropout": 0.5,
  "optimizer": "Adam",
  "loss": "dice_loss",
  "metrics": ["dice_coef"],
  "model": "unet",
  "base": 16,
  "input_shape": [256, 256, 1],
  "data_path": "/Lab1/Lab3/X_ray/",
  "test_size": 0.2,
  "last_layer_units": 1,
  "last_layer_activation": "sigmoid",
  "binarize_values": [255],
  "generator": {
    "rotation_range": 10,
    "width_shift_range": 0.1,
    "height_shift_range": 0.1,
    "horizontal_flip": true,
    "zoom_range": 0.2
  }
}